<CiscoIPPhoneMenu>
<Title>Filter Call History</Title>
<MenuItem>
<Name>All</Name>
<URL>http://{{Host}}/xml/en/Cisco/listcallhistory.xml?pagenumber=1&ShowByDirection=&ShowByUser=null&ShowByDate=&any=&hasattachment=off&calldate=&morethan=0&submit=search1</URL>
</MenuItem>
<MenuItem>
<Name>Inbound</Name>
<URL>http://{{Host}}/xml/en/Cisco/listcallhistory.xml?pagenumber=1&ShowByDirection=in&ShowByUser=null&ShowByDate=&any=&hasattachment=off&calldate=&morethan=0&submit=search1</URL>
</MenuItem>
<MenuItem>
<Name>Outbound</Name>
<URL>http://{{Host}}/xml/en/Cisco/listcallhistory.xml?pagenumber=1&ShowByDirection=out&ShowByUser=null&ShowByDate=&any=&hasattachment=off&calldate=&morethan=0&submit=search1</URL>
</MenuItem>
<MenuItem>
<Name>Missed</Name>
<URL>http://{{Host}}/xml/en/Cisco/listcallhistory.xml?pagenumber=1&ShowByDirection=missed&ShowByUser=null&ShowByDate=&any=&hasattachment=off&calldate=&morethan=0&submit=search1</URL>
</MenuItem>

<SoftKeyItem>
<Name>Select</Name>
<URL>SoftKey:Select</URL>
<Position>1</Position>
</SoftKeyItem>
<SoftKeyItem>
<Name>Back</Name>
<URL>SoftKey:Exit</URL>
